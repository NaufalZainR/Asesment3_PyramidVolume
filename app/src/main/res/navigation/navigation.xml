<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/hitungFragment">
    <fragment
        android:id="@+id/hitungFragment"
        android:name="org.d3if2009.pyramidvolume.ui.hitung.HitungFragment"
        android:label="Hitung Pyramid"
        tools:layout="@layout/fragment_hitung" />
    <action
        android:id="@+id/action_hitungFragment_to_aboutFragment"
        app:destination="@id/aboutFragment" />
    <action
        android:id="@+id/action_hitungFragment_to_pyramidFragment"
        app:destination="@id/pyramidFragment" />
    <action
        android:id="@+id/action_hitungFragment_to_historiFragment"
        app:destination="@id/historiFragment" />
    <fragment
        android:id="@+id/aboutFragment"
        android:name="org.d3if2009.pyramidvolume.ui.AboutFragment"
        android:label="About"
        tools:layout="@layout/fragment_about"/>
    <fragment
        android:id="@+id/pyramidFragment"
        android:name="org.d3if2009.pyramidvolume.ui.pyramid.PyramidFragment"
        android:label="Pyramid di Dunia"
        tools:layout="@layout/fragment_pyramid"/>
    <fragment
        android:id="@+id/historiFragment"
        android:name="org.d3if2009.pyramidvolume.ui.histori.HistoriFragment"
        android:label="@string/histori"
        tools:layout="@layout/fragment_histori" />
</navigation>